        <trans-unit id="CurrentImageInfo" translate="yes" xml:space="preserve">
          <source>Current picture: {0}</source>
          <target state="new">Current picture: {0}</target>
        </trans-unit>
        <trans-unit id="FixedRatio.Content" translate="yes" xml:space="preserve">
          <source>Fixed ratio</source>
          <target state="new">Fixed ratio</target>
        </trans-unit>
        <trans-unit id="FixedSize.Content" translate="yes" xml:space="preserve">
          <source>Fixed Size</source>
          <target state="new">Fixed Size</target>
